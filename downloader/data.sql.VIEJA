INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1364", "poten.gif", "Potencias y raíces cuadradas","Secundaria", "Matemáticas", "Paquete de actividades Clic en el que se trabaja el concepto de potencia y el algoritmo de resolución manual de la raíz cuadrada, dirigido a 1º de ESO. El paquete contiene actividades explicativas, de asociación, de elección de opciones, puzzles, etc.", "Español", "poten/poten.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1365", "funcione.gif", "Funciones","Secundaria", "Matemáticas", "Conjunto de actividades sobre funciones para Educación Secundaria. El paquete está dividido en 4 niveles, de 1º a 4º de ESO, con actividades de: localización de puntos en un plano cartesiano. interpretación de una gráfica. identificación de diferentes tipos de rectas. cálculo de la pendiente de una recta con distintos procedimientos. saber escribir la ecuación de una recta en sus diferentes formas. ejercicios con parábolas, funciones exponenciales e hiperbólicas.", "Español", "funcione/funcione.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1366", "serisenz.gif", "Sèries numèriques senzilles","Primària", "Matemàtiques", "Paquet d'activitats de matemàtiques per a educació primària. L'activitat, per treballar el càlcul mental, es divideix en dues parts on els nens i nenes poden triar activitats amb nombres de l'1 al 20 o bé de l'1 al 100. També poden triar la dificultat de les sèries, n'hi ha de tres tipus: sumar restar sumar i restar", "Català", "serisenz/serisenz.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1366", "serisenz.gif", "Sèries numèriques senzilles","Primaria (6-12)", "Matemáticas", "Paquete de actividades de matemáticas para educación primaria. La actividad, para trabajar el cálculo mental, se divide en dos partes dónde los niños y niñas pueden realizar con números del 1 al 20 o bien del 1 al 100. También pueden elegir la dificultad de las series, hay de tres tipos: sumar restar sumar i restar", "euskara", "seriseus/seriseus.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1367", "euro6.gif", "El Euro","Primaria, Secundaria", "Matemáticas", "Paquete de 63 actividades sobre el euro y países del Euro. La aplicación contiene 6 bloques diferentes: Información general sobre el Euro. Información monedas. Información billetes. Países del Euro. Actividades monedas y billetes. Problemas Euro.", "Español", "euro6/euro6.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
