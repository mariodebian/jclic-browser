INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1093", "meditpla.gif", "Formaciones vegetales del litoral mediterráneo","Secundaria", "Ciencias experimentales", "En este paquete de actividades se trabajan las formaciones vegetales más importantes del litoral mediterráneo. Empieza con unos conceptos básicos sobre la identificación de las plantas y presenta a continuación las especies propias de los dos hábitats más frecuentes en el Mediterráneo: zonas rocosas y arenales.", "Español", "meditpla/meditpla.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1094", "eguzkis.gif", "Eguzki sistema","Primaria", "Ciencias experimentales", "Paquete de actividades sobre el sistema solar en eusquera. El paquete está destinado a alumnos de 6º de Primaria. Encontramos actividades de identificación, de asociación, rompecabezas, de texto...", "Vasco", "eguzkis/eguzkis.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1095", "celula.jpg", "La Célula","Secundaria", "Ciencias experimentales", "Este paquete de actividades ha sido pensado para comprender las estructuras y funciones celulares, cuya actividad origina todos los procesos que vemos a diario en todos los organismos vivos.", "Español", "celula/celula.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1096", "atmosfe2.jpg", "Temps de temps: L'atmosfera","Secundària", "Ciències experimentals", "Aquest primer paquet de la sèrie dedicada a la meteorologia se centra en l'estudi de l'atmosfera i els fenòmens atmosfèrics més comuns. El primer grup d'activitats es dedica a estudiar l'estructura, composició i funcionament de l'atmosfera, per tal de veure com aquesta capa de gasos i la seva interrelació amb les radiacions solars determinen l'existència del clima i les variacions climàtiques al nostre planeta. El segon grup d'exercicis vol aconseguir que l'alumne arribi a un coneixement sistematitzat dels meteors més comuns. En aquestes pàgines es troben les altres parts de la sèrie temps de temps: Observem i mesurem, La previsió meteorològica, A bots i barrals i Llamp de rellamp.", "català", "atmosfe2/atmosfe2.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1097", "ocells.gif", "Els ocells del pati","Primària, Secundària", "Ciències experimentals", "Són una sèrie d¿activitats interdisciplinars que pretenen mostrar i ensenyar a identificar les aus que tenim a prop nostre i que conviuen amb nosaltres al pati de l¿escola i també a la nostra ciutat o poble. Organitzat en vuit paquets: Identifiquem els ocells: Informació sobre el nom i el cant dels ocells. Identificacions, relacions i associacions entre l¿ocell i el seu nom i cant. Coneixem millor els ocells: Identificació en làmines, parts de l¿ocell, dimorfisme sexual, les plomes: parts i tipus, el niu, capsa-niu, la migració, trencaclosques diversos. Sopes d¿ocells: Localització dels nom dels ocells en diferents sopes en ordre de dificultat, amb el reforç de la imatge i el cant. Refranys, expressions, dites i maneres de dir: Activitats de llengua basades en refranys, dites, expressions, maneres de dir, trencaclosques d¿ordenar refranys. Juguem amb els ocells: Jocs de diferències, comptar ocells, identificar siluetes, paraules creuades, ocell de paper. Juguem amb les paraules: Activitats de llengua i literatura amb cançons, poemes, escriptura correcta del nom dels ocells. Fem puzzles: Trencaclosques de diferents tipus. Guia i cants dels ocells: Informació sobre ocells, consells d¿ús de la guia, terminologia de les parts d¿un ocells, ús de prismàtics, 31 fitxes de descripció dels ocells del pati amb la imatge i el seu cant. La instal·lació inclou una guia didàctica sobre el paquet.", "Català", "ocells/ocells.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1097", "ocells.gif", "Los pájaros del patio","Primaria, Secundaria", "Ciencias experimentales", "Estas actividades son una serie de actividades interdisciplinares que pretenden mostrar y enseñar a identificar las aves que tenemos cerca nuestro y que conviven con nosotros en el patio de la escuela o en nuestra ciudad o pueblo. La actividad está organizada en 8 paquetes: Identificamos las aves: Información sobre el nombre y el canto de los pájaros. Identificaciones, relaciones y asociaciones entre el ave y su nombre y canto. Conozcamos mejor las aves: Identificación en láminas, partes de las aves, dimorfismo sexual, las plumas: partes y tipos, el nido, cajas-nido, la migración, puzzles diversos. Sopas de aves: Localización de los nombres de aves en diferentes sopas de letras de diferente dificultad, con el refuerzo de la imagen y el canto. Refranes, expresiones y dichos: Actividades de lengua basadas en refranes, dichos, expresiones y puzzles de ordenar refranes. Jugamos con las aves: Juegos de diferencias, contar pájaros, identificar siluetas, crucigramas, pájaro de papel. Jugamos con las palabras: Actividades de lengua y literatura con canciones, poemas, escritura correcta del nombre de los pájaros. Hacemos puzzles: Puzzles de diferentes tipos. Guía y cantos de las aves: Información sobre aves, consejos de uso de la guía, terminología de las partes de los pájaros, uso de prismáticos, 31 fichas de descripción de las aves del patio con la imagen y su canto. En la instalación se incluye una guía didáctica de uso del paquete.", "español", "pajaros/pajaros.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1098", "botanica.jpg", "El mundo de las plantas","Secundaria", "Ciencias experimentales", "Aplicación sobre el mundo de las plantas, destinada a alumnado de primer ciclo de ESO y programas de Diversificación Curricular. Incluye cuatro paquetes de actividades, dos de ellos trabajan conceptos básicos sobre partes de la flor, tipos de hojas, clasificación de las plantas, fotosíntesis, reproducción vegetal, etc. Otro de los paquetes facilita el reconocimiento de distintas especies de árboles, flores y arbustos comunes. El último de ellos se refiere a las plantas y la salud.", "Español", "botanica/botanica.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1099", "algas.jpg", "Las algas","Secundaria", "Ciencias experimentales", "Paquete de actividades sobre los diferentes tipos de algas que encontramos en el mar Mediterráneo: verdes rojas pardas praderas De cada una de ellas se estudia: las partes, la morfología y donde crecen.", "Español", "algas/algas.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
