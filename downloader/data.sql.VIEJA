INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1212", "esmorzar.gif", "Esmorzar: energia per treballar","Primària", "", "Conjunt d¿activitats que pren el tema de l¿esmorzar com a centre d¿interès per estimular la lectura i plantejar exercicis de comprensió de textos. A la vegada, intenta que els nostres nens i nenes tinguin uns bons hàbits alimentaris i que vegin la necessitat de fer un bon esmorzar. Té tres paquets, d¿unes 25 activitats cadascun, adreçats a tres nivells diferents (Cicle Inicial, Mitjà i Superior) de Primària. S'ha fet a partir d'una llicència retribuïda atorgada pel Departament d'Ensenyament i continua la sèrie de paquets Activitats de lectura comprensiva, Futbol i Animalmania, dels mateixos autors.", "Català", "esmorzar/esmorzar.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1212", "esmorzar.gif", "Desayunar, energía para trabajar","Primaria", "", "Conjunto de actividades que usa el tema del desayuno como centro de interés para estimular la lectura y plantear ejercicios de comprensión de textos. A la vez, intenta que nuestros niños y niñas tengan unos buenos hábitos alimenticios y que vean la necesidad de comer bien. Tiene tres paquetes, de unas 25 actividades cada uno, dirigidos a tres niveles diferentes (Ciclo inicial, medio y superior) de primaria. Se ha hecho a partir de una licencia retribuida otorgada por el Departament de Ensenyament y continua la serie de paquetes Activitats de lectura comprensiva, Futbol y Animalmania, de los mismos autores.", "Español", "desayuno/desayuno.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1213", "qhe.gif", "Quina hora és?","Primària", "", "Quina hora és, és un conjunt de 82 activitats per treballar els aspectes relacionats amb la mesura del temps; element integrant de la programació de matemàtiques del cicle mitjà d'educació primària. L¿activitat consisteix en una seqüència de tres paquets en els quals es desenvolupen de manera sistemàtica aspectes com: el rellotge, les seves parts, les unitats petites de mesura del temps, els diferents tipus de rellotges, la forma correcta de llegir l¿hora en català, les diferents maneres d¿escriure l¿hora (incloent-hi la forma matemàtica o digital) i una bateria de problemes específics sobre el tema. Malgrat que el tema correspon específicament a l¿àrea de matemàtiques, s¿ha volgut incidir especialment en els aspectes relacionats amb la llengua. La dificultat de la lectura correcta de l¿hora en català, unit amb la potència del castellà, ha fet que es dediquin moltes activitats sobre aquests aspectes. S¿ha treballat el vocabulari específic i, mitjançant les activitats d¿escriptura, s¿ha volgut insistir sobre l¿ortografia. Vull expressar el meu agraïment a Javier Rodríguez, de l¿escola l¿Alzina, de Molins de Rei, per autoritzar-me a poder fer ús de les imatges creades per ell en el seu treball El Rellotge.", "Català", "qhe/qhe.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1213", "qhe.gif", "¿Qué hora es?","Primaria", "", "Conjunto de actividades para trabajar los aspectos relacionados con la medida el tiempo, elemento integrante de la programación de matemáticas del ciclo medio de educación primaria. La actividad consiste en un secuencia de tres paquetes en los cuales se desarrollan de manera sistemática aspectos como: el reloj, sus partes, las unidades pequeñas de medida del tiempo, los diferentes tipos de reloj, la manera correcta de leer la hora en Catalán, las diferentes maneras de escribir la hora (inclusive la forma matemática o digital) y una batería de problemas específicos sobre el tema. Aunque el tema corresponde específicamente a el área de matemáticas, se ha querido incidir especialmente en los aspectos relacionados con la lengua. La dificultad de la lectura correcta de la hora en catalán, unida a la potencia del castellano, ha hecho que se dediquen muchas actividades a estos aspectos. Se trabaja el vocabulario específico y, mediante las actividades de escritura, se ha querido insistir sobre la ortografía. Quiero expresar mi agradecimiento a Javier Rodríguez, de la escuela l'Alzina, de Molins de Rei, por autorizarme a hacer uso de las imágenes creadas por él para su trabajo, El Reloj.", "Español", "relojes/relojes.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1213", "qhe.gif", "¿Qué hora es?","Primaria", "", "Conjunto de actividades para trabajar los aspectos relacionados con la medida el tiempo, elemento integrante de la programación de matemáticas del ciclo medio de educación primaria. La actividad consiste en un secuencia de tres paquetes en los cuales se desarrollan de manera sistemática aspectos como: el reloj, sus partes, las unidades pequeñas de medida del tiempo, los diferentes tipos de reloj, la manera correcta de leer la hora en Catalán, las diferentes maneras de escribir la hora (inclusive la forma matemática o digital) y una batería de problemas específicos sobre el tema. Aunque el tema corresponde específicamente a el área de matemáticas, se ha querido incidir especialmente en los aspectos relacionados con la lengua. La dificultad de la lectura correcta de la hora en catalán, unida a la potencia del castellano, ha hecho que se dediquen muchas actividades a estos aspectos. Se trabaja el vocabulario específico y, mediante las actividades de escritura, se ha querido insistir sobre la ortografía. Quiero expresar mi agradecimiento a Javier Rodríguez, de la escuela l'Alzina, de Molins de Rei, por autorizarme a hacer uso de las imágenes creadas por él para su trabajo, El Reloj.", "Vasco", "zod/rellotg0.jclic.zip");

