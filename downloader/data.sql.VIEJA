INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1019", "asocia.gif", "Asociación cognitiva","Infantil", "Diversos", "Programa para trabajar con alumnos con Síndrome de Down, y con cualquier grupo de alumnos en los inicios de la lectoescritura. Se basa en un aprendizaje mediado para que el alumno vaya discriminando imagen, sonido y grafía. Método practicado por el colectivo de Huesca, en tarjetas, para el inicio de la asociación discriminativa, aún no sabiendo leer pero sí asociar. El paquete se elaboró después de asistir a un Curso organizado por la Asociación de Síndrome de Down de Las Palmas de Gran Canaria.", "Español", "asocia/asocia.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1020", "oblog.gif", "Observación, lógica y razonamiento","Primaria, Secundaria", "Diversos", "Paquete de actividades en los que se ejercita la observación, la lógica y el razonamiento mediante ejercicios de comparación, de deducción, de asociación, rompecabezas, secuencias lógicas, etc. Consta de tres paquetes de actividades con un grado progresivo de dificultad.", "Español", "oblog/oblog.jclic.zip");

