INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1247", "supbus.gif", "Super Bus 1","Primaria", "Lenguas", "Paquete de actividades para el aprendizaje de la lengua inglesa para alumnos catalanohablantes, diseñadas como material complementario al libro 'Super Bus 1' de la editorial Heinemann. El paquete se divide en 8 partes donde se trabajan diferentes temas del libro: los colores, el cuerpo humano, números, prendas de vestir, el cuento de Caperucita roja, cosas de clase, las horas, etc. Las actividades van combinando la lengua catalana y la lengua inglesa para ser así más entendedoras.", "español", "supbesp/supbesp.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1248", "catsise.gif", "Exercicis de català, 6è","Primària", "", "Aquesta aplicació Clic va adreçada a segon curs de Cicle Superior d'Educació Primària. Les activitats s'han realitzat a partir del llibre Quadern d'Ortografia 24. El seu objectiu és arribar a assolir els continguts d'ortografia catalana d'una manera més agradable i motivadora utilitzant com a eina l'ordinador.", "Català", "catsise/catsise.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
