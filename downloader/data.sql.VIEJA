INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("3080
1260", "medici.jpg", "The animal","Primària", "", "Paquet d'activitats en anglès sobre els noms dels animals. Al paquet trobem activitats d'associació, trencaclosques, de text, sopes de lletres i mots encreuats.", "Català", "animals2/animals2.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1261", "ordenalf.gif", "Orden alfabético","Primaria, Secundaria", "", "Este paquete sobre el orden alfabético se agrupa en 9 bloques de actividades que presentan una dificultad progresiva. Está pensado para que puedan trabajar en él desde 2º de E.P. hasta los primeros cursos de E.S.O. Se utilizan 460 palabras distintas en las diversas actividades. Cada alumno puede trabajar de forma continuada y progresiva, ampliando su práctica, con el único límite que impone la finalización del paquete. Con este paquete se pretende que trabajen desde la sencillez de conocer el orden de las letras en el alfabeto hasta poder ordenar alfabéticamente grupos de palabras que tienen un comienzo similar. Se aconseja que los alumnos tengan a su alcance papel y lápiz donde puedan anotarse las palabras que deben ordenar, y, en el 9º apartado de pasatiempos, se hace imprescindible su utilización.", "Español", "ordenalf/ordenalf.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1262", "siltoacc.gif", "Síl.labes, tonificació i accentuació","Primària", "", "Paquet d'activitats de llengua catalana format per tres blocs temàtics: Síl·labes: comptar síl·labes, separació de síl·labes, els dígrafs i tipus de paraules segons el nombre de síl·labes. Tonificació: síl·labes tòniques i àtones i tipus de paraules segons la síl·laba tònica. Regles d¿accentuació: tipus d¿accent, paraules agudes, planes i esdrúixoles, regles d¿accentuació, el diftong, la dièresi i l¿accent diacrític.", "Català", "siltoacc/siltoacc.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1263", "otsaze.gif", "Otsoa eta Azeria","Primaria", "", "Trabajo de lengua vasca basado en el sistema pedagógico Amara Berri. Apoyándose en la lectura de un cuento se realizan diversas actividades de: comprensión ortografía ts, tx, tz, z, s, etc. redacción el cuento y sus partes uso de las partículas Omen y Ote verbos sintéticos", "Vasco", "otsaze/otsaze.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1264", "brb3.gif", "What are they doing?","Primary school", "", "Clic activities about English grammar destined to Primary School. The objective is practice the Present Continuous: The package has the following contents: Verbs: write, dance, play, laugh, swim, read, run, rain. Personal pronouns. Sentences in present continuous with the 8 previous verbs. Oral and written structure of sentences and pronouns. Yes/No questions and Short answers. Some images are from the book 'Big Red Bus 3', by Ma José Lobo and Pepita Subirà (Ed. McMillan - Heinemann).", "English", "brb3/brb3.jclic.zip");

