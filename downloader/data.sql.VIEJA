INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1369", "pasmates.gif", "Pasatiempos matemáticos 1","Primaria", "Matemáticas", "Conjunto de actividades en forma de pasatiempos para Educación Primaria del área de matemáticas. En el paquete encontramos ejercicios sobre: Series numéricas. Identificación de objetos iguales. Crucigramas de números. Cuántos cubos forman una figura. Escritura de números.", "Español", "pasmates/pasmates.jclic.zip");

