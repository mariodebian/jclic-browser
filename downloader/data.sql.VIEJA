INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1132", "juegolec.gif", "JuegoLec","Infantil, Primaria", "", "Actividades de lectoescritura en castellano con asociaciones entre palabras, imágenes y sonidos. Se trabajan las letras B, L e Y en dos paquetes distintos: uno en letra manuscrita y el otro en tipos de imprenta. Existe también la versión completa de las actividades: para más información enviar un mensaje de correo electrónico a los autores.", "Español", "juegolec/juegolec.jclic.zip");

