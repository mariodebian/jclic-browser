INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("2010", "xanrata.gif", "La cueva de Xanrata","Primaria", "Diversos", "Este paquete de actividades ha sido creado a partir de un cuento ilustrado por alumnos de 3º y 4º de primaria sobre dos relatos de la tradición oral de la zona, contados por dos abuelos de alumnos. El paquete empieza con la narración del cuento y continua con diversas actividades (asociaciones, rompecabezas, sopas de letras, texto) que buscan el disfrute en la lectura y una mejor comprensión del texto. Encontraréis una descripción más amplia de la actividad en la web del proyecto Fabulando.", "español", "xanrata/xanrata.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
