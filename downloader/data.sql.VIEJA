INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1076", "orienta.gif", "La orientación","Primaria", "Ciencias experimentales", "Paquete de actividades de Clic sobre la orientación basado en el libro de texto 'Conocimiento del medio' de 4º de primaria de la Editorial SC. Las actividades intentan acercar al alumno al conocimiento de los diferentes elementos de orientación terrestre. Se trabajan temas como los movimientos de rotación y translación, los puntos cardinales, los paralelos y meridianos, la brújula, etc.", "Español", "orienta/orienta.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
