INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1372", "musprim.gif", "La música i l'ordinador","Primària, Secundària", "Música", "Conjunt d'activitats on es treballa el llenguatge musical (per nivells i per temes), els instruments musicals, la cobla i l'orquestra, i activitats d'audició. Conté també un paquet d'activitats d'avaluació sobre els temes treballats. Aquests materials corresponen a la unitat de programació 'La música i l'ordinador'.", "català", "musprim/musprim.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1373", "musica.gif", "Activitats de música per a primària i ESO","Primària, Secundària", "Música", "Cinc paquets d'activitats que utilitzen els recursos sonors del Windows (so digital i MIDI) per treballar diferents aspectes del currículum de música: 'Llenguatge musical', 2Formes musicals', 'Pràctica instrumental', 'Els instruments de l'orquestra' i 'La música en la història'.", "Català", "musica/musica.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
