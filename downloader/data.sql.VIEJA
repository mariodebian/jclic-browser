INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1178", "llengua2.gif", "Activitats de llengua catalana","Secundària", "", "Nova versió amb més de 1000 activitats! Paquets d'activitats d'exercitació de llengua catalana organitzats en dos blocs: Ortografia Vocalisme, accentuació, diacrític, dièresi, B/V, H, P/T/C, B/D/G, L/LL/L·L, esses, palatals i M/N. Morfologia Article, apòstrof, gènere, numerals, nombre, morfologia verbal, pronoms febles, relatius, barbarismes, lèxic i calaix de sastre. L'aplicació es complementa amb un paquet de sopes de lletres i un altre de mots encreuats.", "Català", "llengua/llengua.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1179", "cervante.gif", "Miguel de Cervantes y el Quijote","Secundaria", "", "Esta aplicación pretende ser un material que anime a la lectura del Quijote. Las actividades versan sobre la vida y la obra de Miguel de Cervantes, aspectos de su biografía y obras poéticas, dramáticas y narrativas que escribió, con una especial atención a 'El ingenioso hidalgo Don Quijote de la Mancha'.", "Español", "cervante/cervante.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1180", "oracom.gif", "La oración compuesta","Secundaria", "", "Paquete de actividades de Clic sobre las oraciones compuestas. Dividido en tres grupos de actividades de acuerdo con los diferentes tipos de oraciones de esta clase: Compuestas por coordinación Compuestas por yuxtaposición Compuestas por subordinación Los paquetes cuentan con muchas actividades de información a parte de las actividades de asociación, sopas de letras, crucigramas y rompecabezas.", "Español", "oracom/oracom.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
