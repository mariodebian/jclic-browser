INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1219", "verbos2.gif", "Los verbos - 2","Primaria, Secundaria", "", "Paquete de actividades Clic en las que se trabajan las conjugaciones de los verbos regulares, irregulares y auxiliares. También incluye un bloque dedicado a trabajar los tiempos verbales y una relación de las conjugaciones de los verbos cantar, estar, tener, traer, caber, poder, comer, ir, volver, creer, jugar, partir, venir, seguir, echar, hacer, ser, haber, decir, saber y oír. Contiene varios documentos en formato Word a manera de instrucciones para cada actividad.", "Español", "verbos2/verbos2.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1220", "rosaflor.gif", "Romance de Rosaflorida","Primaria", "", "Paquete de actividades sobre el romance de Rosaflorida: En Castilla hay un castillo que se llama Rocafrida al castillo llaman Roca y a la fonte llaman Frida... La aplicación consta de cuatro bloques. En los tres primeros se trabajan las tres partes en que se ha dividido el romance, y en el último hay un resumen y actividades sobre la historia completa.", "Español", "rosaflor/rosaflor.jclic.zip");

