INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1374", "ritme.gif", "Activitats de ritme","Primària, Secundària", "Música", "Conjunt de cinc paquets d'activitats on es treballen ritmes de 3 i 4 pulsacions: 'Activitats per conèixer les figures', 'Activitats de pulsació', 'Activitats de ritmes senzills', 'Activitats de ritmes II' i 'Reconeixem el compàs'. Conté una gran quantitat de fitxers MIDI de temes coneguts, amb arranjaments que permeten identificar-hi les pulsacions rítmiques.", "Català", "ritme/ritme.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1374", "ritme.gif", "Actividades de ritmo","Primaria, Secundaria", "Música", "Conjunto de cinco paquetes de actividades donde se trabajan ritmos de 3 y 4 pulsaciones: 'Actividades para conocer las figuras', 'Actividades de pulsación', 'Actividades con ritmos sencillos', 'Actividades de ritmos II' y 'Reconocimiento de compases'. Contiene una gran cantidad de archivos MIDI de temas conocidos, con arreglos que permiten identificar las pulsaciones rítmicas.", "Español", "ritmo/ritmo.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1375", "britten.gif", "Britten","Primària, Secundària", "Música", "Activitats sobre els instruments de l'orquestra i anàlisi d'una fuga, basades en l'obra 'Guia d'orquestra per a joves' (variacions i fuga sobre un tema de Purcell, op. 34) de Benjamin Britten (1913-1976). Inclou una guia en format Acrobat (PDF) Convé tenir la pantalla configurada a 256 colors", "Català", "britten/britten.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1375", "britten.gif", "Britten","Primaria, Secundaria", "Música", "Actividades sobre los instrumentos de la orquesta y análisis de una fuga basadas en la obra 'Guía de orquesta para jóvenes' (variaciones y fuga sobre un tema de Purcell, op. 34) de Benjamin Britten (1913-1976). Incluye una guía en formato Adobe Acrobat Conviene tener la pantalla configurada a 256 colores", "Vasco", "briteus/briteus.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1375", "britten.gif", "Britten","Primaria, Secundaria", "Música", "Actividades sobre los instrumentos de la orquesta y análisis de una fuga basadas en la obra 'Guía de orquesta para jóvenes' (variaciones y fuga sobre un tema de Purcell, op. 34) de Benjamin Britten (1913-1976). Incluye una guía en formato Adobe Acrobat Conviene tener la pantalla configurada a 256 colores", "Español", "brittesp/brittesp.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1376", "lacobla.jpg", "La cobla","Primària, Secundària", "Música", "El paquet està organitzat en quatre seccions: 'La cobla', 'Els instruments', 'Les famílies' i 'Endevina instruments'. S'hi presenten les característiques dels diferents instruments de la cobla, amb fotografies en color, articles de text i enregistraments sonors. Es proposen també un seguit d'exercicis per a repassar els conceptes treballats.", "Català", "lacobla/lacobla.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1377", "flauta.gif", "La flauta Màgica","Primària, Secundària", "Música", "Paquet d'activitats sobre l'òpera de Mozart 'La flauta Màgica'. S'hi treballen l'argument, la música i els personatges de l'obra. Utilitza petits fragments de la música de Mozart per il·lustrar els continguts. Les activitats són variades: puzzles, sopes de lletres, preguntes i respostes...", "Català", "flauta/flauta.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1377", "flauta.gif", "La flauta Màgica","Primaria, Secundaria", "Música", "Paquete de actividades sobre la ópera de Mozart 'La flauta Mágica'. En este se trabajan el argumento, la música y los personajes de la obra. Utiliza pequeños trozos de la música de Mozart para ayudar a explicar los contenidos. Las actividades son variadas: puzzles, sopas de letras, preguntas y respuestas...", "Español", "flautaes/flautaes.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1378", "carnaval.gif", "Carnaval dels animals","Primària", "Música", "Paquet d'activitats on es presenten deu escenes de l'obra de Camile Saint-Säens 'Carnaval dels animals': El lleó, Galls i gallines, Zebres, Tortugues, L'elefant, Cangurs, L'aquàrium, Els orelluts, El cucut, Ocells, Pianistes, Fòssils, El cigne i Final. En cada escena es treballen els personatges, els instruments que hi intervenen i altres conceptes musicals: escales, ritmes, frases... Conté una gran quantitat d'imatges, sons i animacions de vídeo digital. Es recomana configurar la pantalla a 800x600 i milions de colors.", "Català", "carnaval/carnaval.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1378", "carnaval.gif", "Carnaval dels animals","Primaria", "Música", "Paquete de actividades en el que se presentan diez escenas de la obra de Camile Saint-Säens 'Carnaval de los animales': El león, Gallos y gallinas, Cebras, Tortugas, El elefante, Canguros, El acuario, Los orejudos, El cuco, Pájaros, Pianistas, Fósiles, El cisne y Final. En cada escena se trabajan los personajes, los instrumentos que intervienen y otros conceptos musicales: escalas, ritmos, frases... Contiene una gran cantidad de imágenes, sonidos y animaciones de vídeo digital. Se recomienda configurar la pantalla a 800x600 y millones de colores.", "Gallego", "carnagal/carnagal.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1378", "carnaval.gif", "Carnaval dels animals","Primaria", "Música", "Paquete de actividades en el que se presentan diez escenas de la obra de Camile Saint-Säens 'Carnaval de los animales': El león, Gallos y gallinas, Cebras, Tortugas, El elefante, Canguros, El acuario, Los orejudos, El cuco, Pájaros, Pianistas, Fósiles, El cisne y Final. En cada escena se trabajan los personajes, los instrumentos que intervienen y otros conceptos musicales: escalas, ritmos, frases... Contiene una gran cantidad de imágenes, sonidos y animaciones de vídeo digital. Se recomienda configurar la pantalla a 800x600 y millones de colores.", "español", "carnaves/carnaves.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1379", "sardana.gif", "La Sardana a l'Escola","Primària", "Música", "Paquet d'activitats Clic sobre la sardana: Què és la Sardana. Quins estils es fan servir per ballar-la. Manifestacions sardanístiques. Què és la cobla. Instruments de la cobla (conèixer el nom i el so). Famílies d¿instruments que hi ha a la cobla. Col·locació dels instruments dins la cobla. Conèixer alguns autors i alguns títols de sardanes. Conèixer algunes lletres de sardanes.", "Català", "sardana/sardana.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1380", "patum.jpg", "La Patum de Berga","Primària, Secundària", "Música", "Aquest paquet d¿activitats té com a finalitat fer conèixer la Patum de Berga. Comença amb un text on s'expliquen les característiques d'aquesta festa i continua amb un conjunt d'activitats on es poden veure fotografies i escoltar la música de les comparses: Turcs i cavallets, Maces, Guites, L'àliga, Els gegants, Nans vells, Nans nous, Plens i Tirabols. El paquet es complementa amb trencaclosques, jocs i sopes de lletres.", "Català", "patum/patum.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1381", "orff.gif", "Els instruments de percussió Orff","Primària", "Música", "Paquet d'activitats on es relacionen els noms, sons i fotografies dels instruments de percussió Orff. Els instruments treballats són: Instruments de fusta Claus, Maraques, Güiro, Castanyoles de mànec, Caixa xinesa i Temple block. Instruments de metall Plats, campana, cròtals, cascavell, sonalla i triangle. Instruments de membrana Bongos, pandero, pandereta, congas, timbal i caixa. Instruments de placa Carilló (soprano i alt), Xilofon (soprano, alt i baix) i Metal·lòfon (soprano, alt i baix)", "Català", "orff/orff.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1381", "orff.gif", "Los instrumentos de percusión Orff","Primaria", "Música", "Paquete de actividades donde se relacionan los nombres, sonidos y fotografías de los instrumentos de percusión Orff. Los instrumentos trabajados son: Instrumentos de madera Claves, Maracas, Güiro, Castañuelas de mango, Caja china y Temple block. Instrumentos de metal Platos, campana, crótalos, cascabel, sonaja y triángulo. Instrumentos de membrana Bongos, pandero, pandereta, congas, tambor y caja. Instrumentos de placa Carillón (soprano y alto), Xilófono (soprano, alto y bajo) y Metalófono (soprano, alto y bajo)", "Español", "orffesp/orffesp.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1381", "orff.gif", "Los instrumentos de percusión Orff","Primaria", "Música", "Paquete de actividades donde se relacionan los nombres, sonidos y fotografías de los instrumentos de percusión Orff. Los instrumentos trabajados son: Instrumentos de madera Claves, Maracas, Güiro, Castañuelas de mango, Caja china y Temple block. Instrumentos de metal Platos, campana, crótalos, cascabel, sonaja y triángulo. Instrumentos de membrana Bongos, pandero, pandereta, congas, tambor y caja. Instrumentos de placa Carillón (soprano y alto), Xilófono (soprano, alto y bajo) y Metalófono (soprano, alto y bajo)", "Vasco", "orffeus/orffeus.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1381", "orff.gif", "Los instrumentos de percusión Orff","Primaria (6-12)", "Música", "Paquete de actividades donde se relacionan los nombres, sonidos y fotografías de los instrumentos de percusión Orff. Los instrumentos trabajados son: Instrumentos de madera Claves, Maracas, Güiro, Castañuelas de mango, Caja china y Temple block. Instrumentos de metal Platos, campana, crótalos, cascabel, sonaja y triángulo. Instrumentos de membrana Bongos, pandero, pandereta, congas, tambor y caja. Instrumentos de placa Carillón (soprano y alto), Xilófono (soprano, alto y bajo) y Metalófono (soprano, alto y bajo)", "gallego", "orffgal/orffgal.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1382", "folkch.gif", "El folklore musical de Chile","Primaria (6-12), Secundaria (12-16)", "Música", "En este paquete de actividades se presentan las formas musicales del folklore Chileno organizadas por zonas: Zona norte, zona central, zona sur y zona insular (isla de Pascua). Entre otras se trabajan la cueca, el trote, el cachimbo el taquirari, la tonada, el corrido, el vals, la música araucana mapuche, la pericona y los cantos de origen polinesio. También se presentan algunos de los instrumentos populares utilizados para interpretar la música.", "español", "folkch/folkch.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1383", "stambak4.gif", "Anàlisi i reproducció d'estructures rítmiques","Primària, Secundària", "Música", "Paquet d'activitats d'Educació Musical per a l'etapa de primària, on es treballa l'anàlisi i la reproducció d'estructures rítmiques. Aquestes activitats estan pensades per 6 nivells: un primer nivell inicial i després cinc nivells més que arriben a treballar amb sis notes. Al final de cada nivell hi ha tres jocs. El paquet inclou les ajudes de dos d'ells (quatre en ratlla i memòria).", "Català", "stambak4/stambak4.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1384", "muspercu.gif", "Instrumentos de pequeña percusión","Primaria", "Música", "Este paquete de actividades pretende acercar a los niños/as de primer y segundo ciclo de Primaria a los instrumentos de pequeña percusión que normalmente tenemos en clase. Cuando los instrumentos estén en clase, se usará como una actividad de apoyo, de refuerzo..., ya que no hay nada como la exploración y la manipulación para conocerlos. Se comenzará realizando actividades directas con los instrumentos de clase, para pasar a realizar las actividades con el Clic. No obstante, habrá escuelas en las que por diferentes causas no podamos contar con estos instrumentos musicales, bien por encontrarse en mal estado, por no tener suficientes recursos... En este caso, este paquete de actividades adquiere un sentido propio, un valor en sí mismo y lo utilizaremos para que los niños/as los conozcan.", "Español", "muspercu/muspercu.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1385", "jugmusi.jpg", "Juguem amb la música","Infantil", "Música", "Paquet d'activitats adreçat a Educació Infantil on treballa el reconeixement de melodies i qualitats del so a partir de les cançons: La lluna, la pruna El tren xicotet Jo tinc una pilota Un peixet en el fons de l'estany Plou i fa sol La tortuga va endavant Peix peixet Sol solet", "Català", "jugmusi/jugmusi.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1386", "musicon.gif", "Musicón","Primària", "Música", "Aquest paquet d'activitats està dissenyat per al primer cicle de primària. Conté 4 paquets d'activitats molt senzilles: El primer tracta de la col·locació de les notes en el pentagrama, alguns instruments de la gran orquestra i la gran clau de sol. En el segon hem de jugar amb la notació musical i reconèixer algunes notes del pentagrama. Al tercer juguem amb un teclat on hem de reconèixer les notes, i finalment interpretar l'himne de l'alegria (segons el nivell dels alumnes pot resultar complicat aquest exercici). Finalment, amb el quart exercici intentarem descobrir les parelles ajudant-nos dels sons que produeixen.", "Català", "musicon/musicon.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1386", "musicon.gif", "Musicón","Primaria (6-12)", "Música", "Este paquete de actividades está diseñado para el primer ciclo de primaria. Contiene 4 paquetes de actividades muy sencillas: El primero trata de la colocación de las notas en el pentagrama, algunos instrumentos de la gran orquesta y la gran clave de sol. En el segundo se juega con la notación musical y el reconocimiento de algunas notas en el pentagrama. En el tercero jugamos con un teclado dónde hemos de reconocer las notas y finalmente interpretar el himno de la alegría (según el nivel de los alumnos puede resultar complicado) Finalmente y con el cuarto ejercicio intentaremos descubrir las parejas ayudándonos del sonido que producen.", "euskara", "musikon/musikon.jclic.zip");

