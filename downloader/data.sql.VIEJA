INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1033", "tecnol.gif", "Activitats de tecnologia","Secundària", "Diversos", "Col·lecció de paquets d'activitats fets pel departament de formació professional adaptada de l'Escola Especial Ntra. Sra. de Montserrat, de Barcelona. Les activitats estan relacionades amb eines (tenalles, alicates, barrines, etc.) per tal de conèixer el seu nom i la seva utilitat, tipus d'encaixos i empiuladures que es poden fer amb fusta, identificació de colors i orientació espacial, marqueteria, etc. L'aplicació consta de sis paquets d'activitats: El nom de les eines Unions de peces de fusta Cargols i tornavisos Més eines... Fem animals de marqueteria Els noms de les eines en català i en castellà", "Català", "tecno/tecno.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
