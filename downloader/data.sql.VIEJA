INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1340", "fraccio2.gif", "Fracciones - 2","Primaria", "Matemáticas", "Continuación del paquete Fracciones - 1 en el que se tratan los temas: - Operaciones con fracciones. - Suma y resta de fracciones con igual denominador. - Suma y resta de fracciones con distinto denominador. - Suma y resta de un número entero y una fracción. - Producto de una fracción por un número entero. - Producto de fracciones. - División de una fracción entre un número entero.", "Español", "fraccio2/fraccio2.jclic.zip");

