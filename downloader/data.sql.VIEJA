INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("2007", "balonces.jpg", "Baloncesto","Primaria, Secundaria", "Diversos", "Esta actividad tiene 4 apartados distintos sobre baloncesto: historia reglamento técnica táctica La actividad no pretende profundizar exhaustivamente en cada parte, sino dar unos contenidos mínimos para que el alumno/a de 10 - 14 años sepa jugar a baloncesto. La actividad dispone de numerosas imágenes recopiladas de distintos lugares que explican pormenorizadamente los objetivos a conseguir. A la vez van acompañadas de ejercicios que facilitan el aprendizaje. Resulta un buen complemento y refuerzo para aquellas explicaciones que el profesor realiza en el patio sobre este deporte en concreto.", "español", "balonces/balonces.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("2008", "jugando.gif", "Jugando aprendo","Infantil", "Diversos", "Aplicación dividida en cuatro áreas de trabajo: Rompecabezas que van incrementando en el nivel de dificultad. Orientación: Izquierda-derecha, identificando dibujos que van en una u otra dirección. Memorizar: Serie de juegos de 'memoria' que van aumentando la cantidad de fichas. Buscar diferencias. Una serie para buenos observadores.", "español", "jugando/jugando.jclic.zip");

