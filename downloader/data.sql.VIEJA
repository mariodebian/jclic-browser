INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1145", "irakurri.gif", "Irakurketa hasteko","Infantil", "", "Irakurketa hasteko programa bat da, Haur Hezkuntzako ikasleentzako zuzendua, 5 eta 4 urteko ikasleeentzat. Programak duen ezagarri nagusia testua eta irudiaren arteko lotura aurkitzea da. Euskeraz eta gasteliaz dago. Es un programa bilingüe eusquera-español de iniciación a la lectura, dirigido al alumnado de educación infantil. Se basa en la relación figura texto. Tiene 4 posibilidades: relacionar figura-texto, ordenar figuras después de leído el texto, buscar las vocales perdidas y dada una figura escribir su nombre.", "VascoEspañol", "irakurri/irakurri.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1146", "irudiadi.gif", "Irudiadi","Primaria, Secundaria", "", "Euskeraz egindako programa bat da eta euskal aditza lantzeko erabil daiteke. Irudien bidez laguntzailearen erabilpena esakatzen da. Es un programa en eusquera para el aprendizaje del verbo en esta lengua. Se usan elementos figurativos para que establezcan el auxiliar correspondiente.", "Vasco", "irudiadi/irudiadi.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1148", "rinxols.gif", "Na Rínxols d'Or","Infantil, Primària", "", "Tres paquets d'activitats sobre el conte de na Rínxols d'Or, adreçades al cicle superior d'educació infantil i l'inicial de primària. En el primer paquet s'explica el conte mitjançant dibuixos, textos i sons, en el segon es proposen alguns trencaclosques fets a partir de les imatges i en el tercer es plantegen activitats de lectura comprensiva.", "Català", "rinxols/rinxols.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1148", "rinxols.gif", "Ricitos de oro","Infantil, Primaria", "", "Tres paquetes de actividades sobre el cuento de Ricitos de Oro, dirigidas al ciclo superior de educación infantil y el inicial de primaria. En el primer paquete se explica el cuento mediante dibujos, textos y sonido, en el segundo se proponen algunos puzzles a partir de las imágenes y en el tercero se plantean actividades de lectura comprensiva.", "Español", "ricitos/ricitos.jclic.zip");

