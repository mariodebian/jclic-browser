INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1182", "hable.gif", "Habilidades de lectura y escritura","Primaria, Secundaria", "", "Actividades de lengua castellana para la formación básica de las personas adultas, dirigidas a los niveles 1 y 2 de la etapa instrumental. En esta aplicación se han integrado tres conjuntos de paquetes de actividades: FrasesTres grupos de paquetes que se basan en la lectura y memorización de una frase de 2, 3, 4, 5 o 6 palabras ¿ según dificultad y progresión de las actividades- y su posterior escritura. Habilidades de lectura y escrituraTres grupos de paquetes en los que de manera progresiva se introducen los distintos grupos silábicos. La grafía utilizada es de letra mecanoescrita, tanto en mayúsculas como en minúsculas. MisceláneaCinco paquetes con propuestas como ordenar el abecedario, los días de la semana y los meses del año, ordenar letras o palabras según el abecedario, clasificar palabras, relacionar palabras... Se incluye un documento descriptivo en formato PDF.", "Español", "hable/hable.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1183", "lensexto.gif", "Lengua castellana - 6º de primaria","Primaria", "", "Conjunto de paquetes de actividades de lengua para 6º de primaria organizados de una manera similar a la que se utiliza en los libros de texto de la editorial Anaya, con los siguientes contenidos: - Conceptos y actividades I: Repaso General - La oración - Las clases de palabras - Las sílabas y la palabra - Conceptos y actividades II: Repaso General - Morfología de la palabra - Complementos del verbo - El verbo y su conjugación - Ortografía: Repaso General - Ortografía: palabras - Ortografía: texto - El verso y la canción", "Español", "lensexto/lensexto.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1184", "adivi.gif", "Adivina adivinanza","Primaria", "", "Paquete de actividades con adivinanzas en castellano. Detrás de cada serie de adivinanzas se plantea una sopa de letras con las palabras que han aparecido.", "Español", "adivi/adivi.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1185", "lequinto.gif", "Lengua castellana - 5º primaria","Primaria", "", "Es un paquete de ESTUDIO, REFUERZO Y REPASO de gran parte de los temas del programa de Lengua Castellana del Tercer Ciclo de Primaria. El menú de actividades es el siguiente: GRAMÁTICAEl Nombre y el Adjetivo - Los Determinantes - Los Pronombres Personales y los Verbos - El Adverbio y la Preposición - Las Palabras, la Oración y un Romance. ORTOGRAFÍALas Sílabas y la Palabra - La Palabra: ortografía - El Texto: ortografía MORFOLOGÍALexemas y morfemas, sufijos y prefijos...", "Español", "lequinto/lequinto.jclic.zip");

