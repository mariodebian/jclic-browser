INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1091", "elecat.jpg", "El elefante y otros grandes animales","Infantil, Primaria", "Ciencias experimentales", "Paquete de actividades destinado a Educación infantil y a primer ciclo de Educación Primaria donde se trabajan los animales grandes: el elefante, el hipopótamo, la jirafa y el rinoceronte. En este paquete encontramos actividades de asociación, de identificación de partes del elefante y rompecabezas. La actividad adjunta un fichero de ayuda donde se explica los tipos de actividades y los niveles a que pertenecen.", "Español", "elecast/elecast.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1092", "dinosaur.gif", "Dinosaures","Infantil, Primària", "Ciències experimentals", "Paquet d'activitats Clic sobre Dinosaures. Pretén ser una senzilla aproximació a aquests antics i magnífics habitants del planeta. Conté activitats de trencaclosques, jocs d¿associació i memòria, així com exercicis per a la comprensió lectora. Pensat per a Cicle Inicial, tot i que pot funcionar bé, amb el degut suport, a Educació Infantil.", "Català", "dinosaur/dinosaur.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1092", "dinosaur.gif", "Dinosaurios","Infantil, Primaria", "Ciencias experimentales", "Paquete de actividades Clic sobre dinosaurios. Pretende ser una sencilla aproximación a estos antiguos habitantes del planeta. Contiene actividades de rompecabezas, asociación, memoria y comprensión lectora. Pensado para el ciclo inicial de primaria, también puede ser utilizado en educación infantil con el debido soporte.", "Español", "dinos/dinos.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
