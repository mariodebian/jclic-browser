INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1405", "petjades.jpg", "Petjades d'home","Secundària", "Ciències socials", "Un passeig per l'evolució de l'espècie humana des dels primers homínids, fa més de 4 milions d'anys, fins a l'homo sàpiens actual. Les activitats proposen diferents tipus d'exercicis per a conèixer i diferenciar set estadis: Australopithecus afarensis Australopithecus africanus Australopithecus robustus Homo habilis Homo erectus Homo sapiens neanderthalensis Homo sapiens sapiens Aquest paquet pertany al conjunt d'aplicacions sobre l'evolució format per Petjades d'home, Canviar per viure, Una gran família, Bricolatge còsmic, Refent el passat i Ombres del passat. Trobareu més informació a la web Petjades d'home: un viatge per la nostra evolució.", "Català", "petjades/petjades.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1405", "petjades.jpg", "Petjades d'home","Secundaria", "Ciencias sociales", "Un paseo por la evolución de la especie humana des de los primeros homínidos hasta el homo sapiens actual. Las actividades proponen distintos tipos de ejercicios para conocer y diferenciar siete estadios: Australopithecus afarensis Australopithecus africanus Australopithecus robustus Homo habilis Homo erectus Homo sapiens neanderthalensis Homo sapiens sapiens Este paquete pertenece al conjunto de aplicaciones sobre la evolución formado por Petjades d'home, Canviar per viure, Una gran família, Bricolatge còsmic, Refent el passat y Ombres del passat. Encontraréis más información en la web Petjades d'home: un viatge per la nostra evolució.", "Vasco", "urrats/urrats.jclic.zip");

