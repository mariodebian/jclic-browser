INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1351", "formak.gif", "Formas","Infantil", "Matemáticas", "Paquete de actividades dirigido al alumnado de Ed. Infantil de 3, 4 y 5 años. La actividad consiste en reconocer las figuras geométricas básicas en objetos del entorno cercano al niño/a. Consta de tres bloques cada uno de ellos específico para los diferentes niveles que integran este ciclo.", "Vasco", "formak/formak.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
