INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1345", "formas.gif", "Formas","Infantil", "Matemáticas", "FORMAS es una aplicación para Educación Infantil. Contiene lenguaje escrito (mínimo) y oral en lengua gallega. Se compone de dos niveles. En el primero hay 22 actividades sobre las 3 formas básicas: círculo, triángulo y cuadrado. En el segundo nivel hay 18 actividades sobre las formas anteriores además de rectángulo y rombo. Con esta aplicación se pretende que el niño se familiarice con las formas básicas que se trabajan en Infantil y mejore su identificación y reconocimiento en objetos de la vida cotidiana.", "Gallego", "formas/formas.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1345", "formas.gif", "Formas","Infantil", "Matemáticas", "FORMAS es una aplicación para Educación Infantil. Contiene lenguaje escrito (mínimo) y oral en lengua gallega. Se compone de dos niveles. En el primero hay 22 actividades sobre las 3 formas básicas: círculo, triángulo y cuadrado. En el segundo nivel hay 18 actividades sobre las formas anteriores además de rectángulo y rombo. Con esta aplicación se pretende que el niño se familiarice con las formas básicas que se trabajan en Infantil y mejore su identificación y reconocimiento en objetos de la vida cotidiana.", "Vasco", "euformas/euformas.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1346", "temps.gif", "Mesurem el temps","Primària", "Matemàtiques", "En aquest paquet d'activitats es treballen els dies de la setmana, els mesos i les estacions de l'any. Els objectius de les activitats són: Saber situar-se en el temps. Saber que ve després de.. Saber que ve abans de... Saber ordenar els dies, els mesos o les estacions començant per qualsevol d'ells. Escriure correctament les dates. Resoldre problemes de dies de la setmana utilitzant la taula del 7 Donat un fet ordenar-lo cronològicament Poder distingir característiques pròpies de cada època.", "Català", "temps/temps.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1346", "temps.gif", "Mesurem el temps","Primaria", "Matemáticas", "En este paquete de actividades se trabajan los días de la semana, los meses y las estaciones del año. Las actividades tiene como objetivo: Saber situarse en el tiempo Saber que viene después de... Saber que viene antes de... Saber ordenar los días, los meses o las estaciones comenzando por cualquiera de ellas Escribir correctamente las fechas Resolver problemas con los días de la semana utilizando la tabla del 7 Poder distinguir características propias de cada época", "Español", "tiempo/tiempo.jclic.zip");

