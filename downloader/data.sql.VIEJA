INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1195", "fira.gif", "Ortografia a la tardor de la fira d'Alcúdia","Primària", "", "Conjunt de 3 paquets d'activitats per a tercer Cicle de Primària amb el següent índex: Diftongs i separació de síl·labes. Partint dels dígrafs divisibles i indivisibles i amb l'ajuda de les definicions de diftong creixent i decreixent, es pretén una correcta separació de les síl·labes d'una paraula. L'apòstrof i les contraccions. Dins un entorn proper i ric en vocabulari com són LES FRUITES DE TARDOR, es troba un material atractiu per a poder consolidar la correcta utilització dels apòstrofs i les contraccions, a la vegada que s'amplia i consolida l'esmentat vocabulari, objectiu primordial d'aquest apartat d'activitats. L'accent gràfic. La vivor, el color i la màgia de la festa, acompanyada d'allò que sense cap dubte, fira rera fira, fascina i captiva als al·lots: S'ESTOL DEL REI EN JAUME (Fira d'Alcúdia), serveix per introduir-se en aquest no poc dificultós àmbit de l'accentuació gràfica, al mateix temps que ens apropa a la nostra realitat històrica - cultural.", "Català", "fira/fira.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1196", "irakurtz.gif", "Irakurtzen eta idazten","Infantil, Primaria", "", "Es un programa en eusquera para apoyar el aprendizaje de la lectura y la escritura. Está dirigido a los alumnos del último curso de Educación Infantil y primer ciclo de Primaria. Irakurketa eta idazketa prozesuan indargarri eta motibagarri izan nahi du programa honek. Dilindan liburuxketan daude oinarriturik eta bertan hainbat silaba lantzen dira. Ariketetan testua eta irudiak irakurri, lotu, identifikatu, idatzi... behar dira. Haur Hezkuntzako azken urteko eta Lehen Hezkuntzako lehenengo zikloko ikasleei dago zuzendurik.", "Vasco", "irakurtz/irakurtz.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1197", "trabatua.gif", "Trabatuak lantzen","Infantil, Primaria", "", "Irakurketa/idazketa prozesuan horrenbeste buruhauste sortzen duten silaba trabatuak lantzeko sortu da programa hau. Haur Hezkuntzako azken urterako eta Lehen Hezkuntza lehenengo zikloko ikasleei dago zuzendurik. Es un programa en eusquera para trabajar las sílabas trabadas dentro del proceso de aprendizaje de la lectura y la escritura. Está dirigido a los alumnos del último curso de Educación Infantil y primer ciclo de Primaria.", "Vasco", "trabatua/trabatua.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
