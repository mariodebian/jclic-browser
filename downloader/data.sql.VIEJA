INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("2022", "trastos.gif", "Temps de temps: Observem i mesurem","Primària", "Ciències experimentals", "Aquest és el segon paquet de la sèrie dedicada a la meteorologia. El paquet comença fent conèixer a l'alumne els elements o variables climatològiques que determinen el clima i tot seguit passa a l'estudi dels aparells meteorològics que permeten mesurar i quantificar aquestes variables. Es tracten el aparells més bàsics: quins són, que mesuren i com funcionen. La finalitat dels dos punts anteriors es aconseguir que l'alumne, a banda d'un coneixement teòric, adquireixi les destreses més elementals per tal de fer-les servir de forma correcta i pugui arribar a un control i seguiment de les dades obtingudes que li permetin fer un estudi climatològic força acurat del seu entorn. En aquestes pàgines es troben les altres parts de la sèrie temps de temps: L'atmosfera, La previsió meteorològica, A bots i barrals i Llamp de rellamp.", "català", "trastos/trastos.jclic.zip");

