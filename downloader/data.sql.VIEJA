INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1164", "pal-llig.gif", "De la lletra de pal a la lletra lligada","Infantil, Primària", "", "Paquet per conèixer i treballar les diferències entre la lletra lligada i la lletra 'de pal'. S'organitza en dos blocs: 'Lletres' i 'Noms'. En el primer es treballen les lletres una per una, i en el segon es presenten els noms dels companys de la classe escrits en els dos tipus de lletra. Per a cada un d'aquests blocs hi ha tres paquets: 'Observa', 'Relaciona' i 'Escriu'. En el bloc 'Noms' hi apareixen els dels nens i nenes de l'escola on s'ha creat el paquet. Per a fer-lo servir correctament cal descompactar-lo (amb el ClicPac) i modificar les activitats per tal que hi apareguin els del grup que l'hagi d'utilitzar.", "Català", "pal-llig/pal-llig.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1164", "pal-llig.gif", "Mayúsculas y minúsculas","Infantil, Primaria", "", "Paquete para conocer y trabajar las diferencias entre la letra ligada y la letra 'de palo'. Se organiza en dos bloques: 'Letras' y 'Nombres'. En el primero se trabajan las letras una por una, y en el segundo se presentan los nombres de los compañeros de clase escritos en los dos tipos de letra. Para cada uno de estos bloques hay tres paquetes: 'Observa', 'Relaciona' y 'Escribe'. En el bloque 'Nombres' aparecen los de los niños y niñas de la escuela en la que se ha desarrollado el paquete. Para utilizarlo correctamente hay que descompactarlo (con el ClicPac) y modificar las actividades para que aparezcan los del grupo al que vaya dirigido.", "Español", "mayusmin/mayusmin.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
