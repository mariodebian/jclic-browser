INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("2012", "gimcainf.gif", "Gimcana del coneixement - Cicle inicial","Primària", "Diversos", "Les dues aplicacions que es presenten en aquesta pàgina formen part d'un conjunt de paquets d'activitats més ampli, adreçat als tres cicles de primària: Cicle inicial: Gimcana infantil 1: Construcció del zoo del parc de l'amistat Gimcana infantil 2: Ajudem a construir una escola Cicle mitjà: Gimcana del coneixement 1: Miscel·lània Gimcana del coneixement 2: El misteriòs geroglífic egipci Cicle superior: Gimcana del coneixement 3: A la recerca del tresor Gimcana del coneixement 4: Una investigació molt personal Aquests paquets d'activitats pretenen dotar al mestre d'una eina que consolidi el currículum que treballa a les diferents àrees a l'aula ordinària. Aquest treball fa una recolecció de la diversitat d'activitats que es realitzen diàriament amb material imprès i el presenta d'una altra manera ben diferent. L'alumne es troba amb una doble vessant davant seu: per una banda es troba una mena de joc-concurs en què ha de demostrar el que ha après o està aprenent; per una altra se li proposa la participació en la consecució d'un objectiu que depén del nivell a qui va dirigit: la construcció d'un Zoo Parc, la Construcció de l'escola de la Solidaritat, el desxiframent d'un jeroglífic Egipci amb caràcters reals, la recerca d'un tresor a una illa del Pacífic o el treball d'un petit detectiu a l'estil de Sherlock Holmes donant la volta al món. Tot això només és possible si es poden resoldre les activitats, doncs el programa no permet continuar a la següent sense haver fet l'anterior. Per al professor les possibilitats d'aprofitament d'aquest material són múltiples: facilita l'atenció a la diversitat i respecta el propi ritme de treball de cada alumne. En cada paquet d'activitats s'inclou un document DOC amb les respostes correctes, per a ús exclussiu del professor/a. El document s'ha d'anar a cercar a la carpeta on queden instal·lades les activitats, ja que no es crea cap icona que l'obri per evitar que els alumnes hi accedeixin.", "català", "gimcain1/gimcain1.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
