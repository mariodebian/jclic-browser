INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1021", "coneixem.gif", "Coneixement del Medi","Primària", "Diversos", "Paquet d'activitats Clic de coneixement del medi social i natural i llengua estrangera (anglès), adreçades al tercer cicle de primària. Les activitats estan dividides en sis paquets que tracten temes diferents: la cèl·lula, la prehistòria, anglès, l'univers i viure en salut.", "Català", "coneixem/coneixem.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1022
1151", "animal.gif", "Animalades","Primària", "Diversos", "Paquet d'activitats Clic sobre els animals adreçat al cicle inicial de primària. Consta de quatre seccions: EscoltaEs mostren els diferents animals i s'escolten els sons que produeixen. LlegeixLectura de poemes curts sobre cada animal. JugaAssociar els sons i els animals. CantaCançons relacionades amb alguns dels animals. El paquet té una gran quantitat de fotografies i sons, i això ha obligat a dividir-lo en deu fitxers d'instal·lació.", "Català", "animal/animalad.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1022", "animal.gif", "Animalades","Primaria", "Diversos", "Paquete de actividades de Clic sobre los animales dirigido al ciclo inicial de primaria. Consta de cuatro secciones: Escucha Se muestran los diferentes animales y se escuchan los sonidos que producen. LeeLectura de poemas cortos acerca de cada animal. JuegaAsociar los sonidos con sus respectivos animales. CantaCanciones relacionadas con algunos de los animales. El paquete tiene una gran cantidad de fotografías y sonidos, y esto ha obligado a dividirlo en diez archivos de instalación.", "Vasco", "abereak/abereak.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1023", "tecladit.gif", "Tecladit","Primària, Secundària", "Diversos", "Tecladit es un programa de mecanografia que ensinistra progressivament l'alumne a la perfecta utilització del teclat, a fer servir tots els dits de les dues mans, a que cada dit 'memoritzi' les tecles que li corresponen i que es pugui escriure sense mirar-les. Es treballen primerament les línies del mig, la superior i la inferior per passar després a l'exercici dels dits exteriors i interiors. Uns petits Controls ens donaran compte de si l'alumne ha superat o no aquella o aquelles lliçons.", "Català", "tecladit/tecladit.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1023", "tecladit.gif", "Tecladit","Primaria, Secundaria", "Diversos", "Tecladit es un programa de mecanografía que enseña progresivamente al alumno la perfecta utilización del teclado para hacer uso de todos los dedos de las dos manos, que cada uno memorice las teclas que le corresponden y que puedan escribir sin mirarlas. Se trabajan primeramente las líneas del medio, la superior y la inferior para pasar después a los ejercicios de los dedos exteriores e interiores. Unos pequeños controles darán cuenta de si el alumno ha superado o no aquella o aquellas lecciones.", "Vasco", "teclaeus/teclaeus.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1023", "tecladit.gif", "Tecladit","Primaria, Secundaria", "Diversos", "Tecladit es un programa de mecanografía que enseña progresivamente al alumno la perfecta utilización del teclado para hacer uso de todos los dedos de las dos manos, que cada uno memorice las teclas que le corresponden y que puedan escribir sin mirarlas. Se trabajan primeramente las líneas del medio, la superior y la inferior para pasar después a los ejercicios de los dedos exteriores e interiores. Unos pequeños controles darán cuenta de si el alumno ha superado o no aquella o aquellas lecciones.", "gallego", "teclaga/teclaga.jclic.zip");

