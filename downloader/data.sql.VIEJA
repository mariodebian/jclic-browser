INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1104", "diabeti2.jpg", "La diabetes tipo I","Primaria, Secundaria", "Ciencias experimentales", "En este segundo paquete de actividades sobre la Diabetes se dan pautas para conocer las complicaciones agudas de esta enfermedad y saber actuar ante ellas, los diferentes tipos de insulina, y valorar la importancia del ejercicio físico como control de la glucemia. Va dirigido a niños y jóvenes que sufren diabetes.", "español", "ediabet2/ediabet2.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1104", "diabeti2.jpg", "La diabetis tipus I: Complicacions agudes, Insulina, Exercici Físic","Primària, Secundària", "Ciències experimentals", "En aquest segon paquet d¿activitats sobre la Diabetis es donen pautes per conèixer les complicacions agudes d¿aquesta malaltia i saber actuar davant d¿elles, els diferents tipus d¿insulina, i valorar la importància de l¿exercici físic com a control de la glucèmia. Va adreçat a infants i joves que pateixen diabetis.", "català", "diabeti2/diabeti2.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1104", "diabeti2.jpg", "La diabetis tipus I","Primary school, Secondary school", "Experimental sciences", "Looking through this second package about Diabetes, we can find some guidelines to know acute complications of this illness and how to act when they come out. It reveals different types of insulin and how important is the physical exercise as well as the glycemic index control.", "English", "adiabet2/adiabet2.jclic.zip");

