INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1166", "comic.jpg", "El llenguatge del còmic","Infantil, Primària", "", "L'aplicació consta de dos paquets d'activitats: un per a educació infantil i l'altre per a primària. S'hi treballen diferents aspectes del llenguatge del còmic: vinyeta, escena, pla, seqüència, angulacions, significat i funció dels signes convencionals (línies cinètiques, metàfores visuals...), la funció de les onomatopeies i reconeixement de personatges. Inclou un document en format PDF amb orientacions per al seu ús.", "Català", "comic/comic.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1166", "comic.jpg", "El llenguatge del còmic","Infantil, Primaria", "", "La aplicación consta de dos paquetes de actividades: uno para educación infantil y otro para primaria. Trabaja distintos aspectos del lenguaje de los comics: viñeta, escena, plano, secuencia, angulaciones, significado y función de los signos convencionales (líneas cinéticas, metáforas visuales...), la función de las onomatopeyas y el reconocimiento de personajes. Incluye un documento en formato PDF con orientaciones para su uso.", "Vasco", "komikia/komikia.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1167", "null", "Adaptació d'activitats Babel","Primària, Secundària", "", "El programa Babel és una aplicació per a MS-DOS que el PIE va adquirir i distribuir a centres de primària i secundària. Una de les novetats del Clic 3.0 són les activitats de text, que permeten passar a un entorn gràfic i multimèdia gairebé tots els exercicis que fins ara s'havien creat amb el programa Babel. Aquests paquets d'activitats són adaptacions directes dels materials Babel que fins ara s'havien publicat al CD-ROM 'Sinera en Disc'. Cal advertir que no aprofiten plenament les noves possibilitats del Clic (dictats amb sons, integració de recursos multimèdia, gràfics, ajudes flotants, modalitats d'avaluació...) ja que el seu objectiu és només donar continuïtat a uns valuosos materials didàctics que fins ara només estaven disponibles per a l'entorn Babel.", "Català", "catsec/catsec.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1167", "null", "Adaptación de actividades Babel","Primaria, Secundaria", "", "El programa Babel es una aplicación para MS-DOS que el PIE adquirió para distribuir a los centros de primaria y secundaria de Cataluña. Una de las novedades de Clic 3.0 son las actividades de texto, que permiten pasar a un entorno gráfico y multimedia prácticamente todos los ejercicios que hasta ahora se habían realizado con el programa Babel. Estos paquetes de actividades son adaptaciones directas de los materiales Babel que hasta ahora se habían publicado en el CD-ROM 'Sinera en Disc'. Hay que advertir que no aprovechan de lleno las nuevas posibilidades de Clic (dictados con sonidos, integración de recursos multimedia, gráficos, ayudas flotantes, modalidades de evaluación...) ya que su objetivo es simplemente dar continuidad a unos valiosos materiales didácticos que hasta ahora solamente estaban disponibles para el entorno Babel.", "Español", "cassec/cassec.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1167", "null", "Adaptació d'activitats Babel","Primària, Secundària", "", "El programa Babel és una aplicació per a MS-DOS que el PIE va adquirir i distribuir a centres de primària i secundària. Una de les novetats del Clic 3.0 són les activitats de text, que permeten passar a un entorn gràfic i multimèdia gairebé tots els exercicis que fins ara s'havien creat amb el programa Babel. Aquests paquets d'activitats són adaptacions directes dels materials Babel que fins ara s'havien publicat al CD-ROM 'Sinera en Disc'. Cal advertir que no aprofiten plenament les noves possibilitats del Clic (dictats amb sons, integració de recursos multimèdia, gràfics, ajudes flotants, modalitats d'avaluació...) ja que el seu objectiu és només donar continuïtat a uns valuosos materials didàctics que fins ara només estaven disponibles per a l'entorn Babel.", "Català", "catpri/catpri.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1168", "vocales.gif", "Las vocales","Infantil", "", "Nueva versión con más de 140 actividades! En este paquete de actividades se trabajan las vocales a partir de sonidos, texto e imágenes. Las instrucciones de cada actividad son mensajes sonoros, para dar la máxima autonomía a los alumnos de educación infantil. En los puzzles y en las asociaciones se ha dejado la bandera y el botón de cambio de puzzle para poder repetir varias veces el ejercicio sin correr el riesgo de aprenderlo mecánicamente y no comprensivamente. En el paquete se incluyen algunas actividades adaptadas de la aplicación El ratón de Margarita Fortuny.", "Español", "vocales/inicio.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1169", "selva.jpg", "El gran llibre de la Selva","Primària", "", "El Gran llibre de la Selva és un treball d'explotació de textos feta amb el Clic 3.0 que tenen com a nexe d'unió una mascota -En Guille- i un determinat territori -pobles de la Selva interior-. Es composa bàsicament de 2 parts: Els textos i dibuixos, realitzats per alumnes dels cicles Mitjà i Superior de 12 CEIPs de la zona Selva-I. que es poden veure també a la WEB del gran llibre de la Selva Les activitats d'explotació dels textos, realitzada pel Centre de Recursos. Al Gran Llibre de la Selva trobareu un munt d'activitats; en concret, 15 lectures, 15 trencaclosques, 15 sopes de lletres i 75 exercicis d'explotació de text.", "Català", "selva/selva.jclic.zip");

