INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1338", "divxfra.gif", "L'Algoritme de la divisió","Primària", "Matemàtiques", "Paquet d'activitats per a practicar l'algoritme de la divisió amb restes pas a pas. En les operacions el dividend té quatre xifres i el divisor només una. Els exercicis es resolen portant les xifres al seu lloc amb el ratolí. En una nova versió ampliada de l'activitat, trobem dues modalitats més de divisions: amb dividends decimals i amb divisors de dues xifres.", "Català", "divxfra/divxfra.jclic.zip");

