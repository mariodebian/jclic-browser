INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("2081", "borja.gif", "Els Borja, mite universal","Secundària", "Ciències socials", "El material gràfic i de text utilitzat en aquest treball està basat en el llibre de Joan Francesc Mira 'Els Borja, família i mite', cedit per edicions Bromera. El paquet s'organitza en 7 parts: El context històric i polític Els orígens Els personatges Els Borja, mecenes de l'art El valencià, llengua dels Borja L'empremta dels Borja en terres valencianes Ara un descanset El paquet d'activitats s'ha realitzat en col·laboració amb la Coordinadora d'Escoles en Valencià de la Ribera.", "català", "borja/borja.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("2082", "reciclar.gif", "Aprenem a reciclar","Infantil, Primària", "Ciències socials", "El paquet ha estat pensat per a treballar amb nens i nenes d¿educació especial, que tot i estar en edat de primària tenen un nivell de lectura nul o en inici només, per això el marc textual és mínim. És per això que també pot adreçar-se a nens i nenes d¿educació infantil. A través de les diferents activitats es pretén guiar als alumnes per tal que vagin coneixent quins dels residus que ells mateixos, a casa o a l¿escola, generen han d¿anar a parar als diferents contenidors. No és només dir ¿aquest contenidor és per a paper...¿ sinó saber que un paquet de cereals, una caixa de sabates o de galetes, etc. han d¿anar a parar a aquest contenidor. De la mateixa manera amb el d¿envasos i vidre. Així es treballa amb quatre paquets: VIDRE ENVASOS PAPER RECICLEM", "català", "reciclar/reciclar.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("2082", "reciclar.gif", "Aprenem a reciclar","Infantil, Primaria", "Ciencias sociales", "El paquete ha estado pensado para trabajar con niños y niñas de educación especial, que estando en edad de primaria tienen un nivel de lectura nulo. El paquete también se puede dirigir a niños y niñas de educación infantil. A través de las diferentes actividades se pretende guiar a los alumnos por tal que vayan conociendo cuáles son los residuos que deben ir a cada contenedor. La actividad consta de 4 partes: Cristal Envases Papel Reciclamos", "euskara", "birzikla/birzikla.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("2083", "transpo2.gif", "Transportes","Infantil, Primaria", "Ciencias sociales", "El programa Transportes surgió para favorecer entre sus alumnos de Educación Infantil y Educación Primaria con especial incidencia en ACNEE(s), aprendizajes específicos como es el conocimiento de los medios de transportes más comunes: terrestres, marítimos y aéreos. El programa consta de un gran número de pantallas que se encaminan a potenciar el aprendizaje de la lectura y su afianzamiento a través del conocimiento de elementos del medio físico y social. Se puede descargar también un fichero de ayuda con una guía didáctica y una guía técnica sobre el paquete.", "español", "transpo2/transpo2.jclic.zip");

