INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1161", "busazul.gif", "El bus azul","Primaria", "", "Actividades de aprendizaje del vocabulario básico castellano para alumnado catalano-parlante. Se estructura en dos bloques: ciclo medio y ciclo superior, y para cada bloque hay diversos paquetes de actividades organizados por temas: los alimentos, el comedor, la cocina, los animales, el cuerpo humano, los animales, la casa... Cada uno de los paquetes de ciclo superior consta de cuatro bloques: vocabulario básico, verbos, frases 1 y frases 2.", "Español", "busazul/busazul.jclic.zip");

