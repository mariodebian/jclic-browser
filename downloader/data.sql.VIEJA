INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1483", "consti.gif", "La constitución para niños","Primaria", "Ciencias sociales", "Paquete de actividades basado en una publicación de la diputación de Palencia y destinado a educación primaria, en el que se trabajan algunos de los contenidos más significativos de la constitución española de 1978: los derechos y deberes de los ciudadanos, la organización en comunidades autónomas, las cámaras de representación, la bandera, el escudo, etc.", "Español", "consti/consti.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
