INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1436", "hisali.gif", "Historia de los alimentos","Primaria", "Ciencias sociales", "Este programa ha sido diseñado a partir del libro de Conocimiento del Medio 'El libro de las preguntas', de 3º de E. primaria, de la editorial Santillana. Consta de tres secciones que tratan sobre la evolución de los alimentos en tres áreas: El cultivo de la tierra Alimentos por la tierra y por el mar La producción en fábricas Encontraréis más información en la web de Aplicaciones educativas de Juan José Mateo.", "Español", "hisali/hisali.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1437", "bricolat.gif", "Bricolatge còsmic","Secundària", "Ciències socials", "BRICOLATGE CÒSMIC s'ocupa de la base genètica de l'evolució i dedica per tant especial atenció a la cèl·lula i a tots els elements i mecanismes implicats en el procés reproductor. Les informacions i activitats es centren sobretot, en els fenòmens de la recombinació gènica i de les mutacions, fets que fan possible tant la variabilitat intraespecífica com l'evolució de les espècies. Aquest paquet pertany al conjunt d'aplicacions sobre l'evolució format per Petjades d'home, Canviar per viure, Una gran família, Bricolatge còsmic, Refent el passat i Ombres del passat. Trobareu més informació a la web Petjades d'home: un viatge per la nostra evolució.", "Català", "bricolat/bricolat.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1438", "bibliote.gif", "Què és i com funciona la biblioteca","Secundària", "Ciències socials", "Activitats sobre la biblioteca: com fer-ne ús, com entendre les fitxes bibliogràfiques, les diferents classificacions bibliotecàries, etc.Les activitats estan dividides en dos blocs: les purament informatives i les que requereixen una resposta per part de l'alumne.", "Català", "bibliote/bibliote.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1439", "medipv.gif", "El País Valencià","Primària", "Ciències socials", "Paquet d'activitats sobre el País Valencià adreçat a l'àrea de coneixement del medi de 5è de primària. Està organitzat en 5 seccions: Les comarquesLocalització, capitals... La culturaLa senyera, l'escut, el govern de la comunitat, tradicions i costums, patrimoni històric, personatges il·lustres... L'economiaConreus, indústria, pesca... La històriaCultures que han viscut al País Valencià al llarg del temps, fets històrics més importants, monuments, personatges... El relleuRius, accidents marítims, cims, serres...", "Català", "medipv/medipv.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1440", "behist.gif", "Història del Baix Empordà","Primària", "Ciències socials", "Aplicació sobre la història del Baix Empordà organitzada en dos paquets: La prehistòriaEtapes de la prehistòria, jaciments paleolítics i neolítics, els megalits... Història antigaLes tribus ibèriques de les comarques gironines, la casa ibèrica, l'herència dels romans...", "Català", "behist/behist.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1441", "elbarrio.gif", "El barrio","Primaria", "Ciencias sociales", "Aplicación basada en el Libro 'Ardilla' (Editorial Edebé) de Conocimiento del Medio para 2º de Ed. Primaria. Consta de 5 paquetes de actividades: El Barrio (11 actividades)los rincones de tu barrio; comparar la calle de un pueblo y una ciudad. La Arquitecta (11 actividades)Elementos del trabajo de la arquitecta; personas que trabajan relacionadas con la arquitecta. Los planos (11 actividades)reconocer elementos de una vivienda dibujados en un plano. La obra(14 actividades)Personas, materiales y herramientas que intervienen en una construcción; los accidentes en el trabajo. Los Edificios (13 actividades)Edificios antiguos y modernos.", "Español", "elbarrio/elbarrio.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1442", "tragones.gif", "El Tarragonès","Primària, Secundària", "Ciències socials", "Paquet d'activitats sobre el Tarragonès que s'organitza en vuit seccions: Comarques meridionals Estudi de les comarques tarragonines. Situació i límits Situació del Tarragonès i comarques limítrofes. Clima i vegetació Els vents, el clima, arbres, arbusts i plantes de la comarca. Relleu, rius i costes El Francolí i el Gaià, el litoral. Pobles i municipis Situació dels 21 municipis del Tarragonès. Recursos econòmics El sectors primari, secundari i terciari. Cultura i tradicions Artistes i personatges, Festes i tradicions, els castellers. Tarragonès monumental Monuments romans i medievals. Tarragona, patrimoni de la humanitat.", "Català", "tragones/tragones.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1443", "etapes.gif", "Les etapes de la història","Secundària", "Ciències socials", "Les activitats sobre 'Les etapes de la història' estan enfocades per a nivells de primer o segon cicle d'ESO. Tracten temes socials, econòmics i polítics a més de visualitzar imatges de cada una de les Etapes: Paleolític, Neolític, Edat antiga, Edat mitjana, Edat moderna i Edat contemporània. És recomana configurar la pantalla a 1024x768", "Català", "etapes/etapes.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1443", "etapes.gif", "Les etapes de la història","Secundaria", "Ciencias sociales", "Las actividades sobre 'Las etapas de la historia' están enfocadas hacia los niveles de primero y segundo ciclo de ESO. Tratan de temas sociales, económicos y políticos además de visualizar imágenes de cada una de las etapas: Paleolítico, Neolítico, Edad antigua, Edad media, Edad moderna y Edad contemporánea. Se recomienda configurar la pantalla a 1024x768", "español", "etapas/etapas.jclic.zip");

