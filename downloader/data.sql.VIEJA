INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1339", "fraccio1.gif", "Fracciones - 1","Primaria", "Matemáticas", "Actividades para 6º de primaria pensadas para realizar en una sesión y que resumen el trabajo en clase de una quincena. En el paquete se trabajan los siguientes contenidos: - Usos y utilidad de las fracciones. - Los términos de la fracción. - Representación gráfica de las fracciones. - Fracciones equivalentes. - Ordenación de fracciones.", "Español", "fraccio1/fraccio1.jclic.zip");

