INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1250", "lengua5.gif", "Lengua castellana 5º","Primaria", "Lenguas", "Paquete de actividades de lengua castellana para 5º de Primaria diseñado como material complementario al libro de texto de la editorial ANAYA. El paquete se compone de 18 temas (como el libro) y está pensado para poder ser realizado por 1 o 2 jugadores.", "español", "lengua5/lengua5.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
