INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1083", "sentits.gif", "Els òrgans dels sentits","Secundària", "Ciències experimentals", "Activitats sobre els cinc sentits i el sistema nerviós: Vista Parts i funcions dels aparells de l'ull, defectes de la visió (miopia i hipermetropia), procés de la visió. Oïda Parts i funcions de l'orella externa, mitjana i interna, procés de l'audició, consells per a mantenir l'oïda sana. Olfacte Òrgans de les fosses nasals, procés de percepció de les olors. Gust Parts de la llengua, els quatre sabors bàsics, procés de percepció del gust. Tacte Capes de la pell i les seves funcions. Glàndules, pèls i ungles. Sistema nerviós Òrgans que formen el sistema nerviós, moviment voluntari i reflex...", "Català", "sentits/sentits.jclic.zip");

INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo)
  VALUES ("1083", "sentits.gif", "Els òrgans dels sentits","Secundaria (12-16)", "Ciencias experimentales", "Actividades sobre los cinco sentidos y el sistema nervioso: Vista Partes y funciones del ojo, defectos de la visión (miopía e hipermetropía), proceso de la visión. Oído Partes y funciones del oído externo, medio e interno, proceso de la audición, consejos para mantener el oído sano. Olfato Órganos de las fosas nasales, proceso de percepción de los olores. Gusto Partes de la lengua, los cuatro sabores básicos, proceso de percepción del gusto. Tacto Capas de la piel y sus funciones. Glándulas, pelos y uñas. Sistema nervioso Órganos que forman el sistema nervioso, movimiento voluntario y reflejo...", "español", "sentidos/sentidos.jclic.zip");

