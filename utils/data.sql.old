, url) VALUES ("1467", "http://clic.xtec.net/gifs/brion.jpg");INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo) VALUES ("1468", "pgitano.gif", "Historia del pueblo gitano", "Primaria", "Ciencias sociales", "En este paquete de actividades se hace una introducción a la historia y la cultura del pueblo gitano y se trabajan algunas palabras de la lengua caló. Está basado en el libro 'Historia del pueblo gitano', elaborado por los departamentos de Orientación, Geografía e Historia, Lengua castellana y Literatura del IES 'Camino de la Miranda' de Palencia.", "Español", "pgitano/pgitano.jclic.zip");INSERT INTO zip_urls (id, url) VALUES ("1468", "http://clic.xtec.net/projects/pgitano/jclic/pgitano.jclic.zip");INSERT INTO img_urls (id, url) VALUES ("1468", "http://clic.xtec.net/gifs/pgitano.gif");INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo) VALUES ("1469", "refent.gif", "Refent el passat", "Secundaria", "Ciències socials", "Paquet d'activitats entorn de la ciència paleontològica. Les activitats s'organitzen en dos grups. El primer, posa a prova els nostres coneixements biològics i geològics a l'hora d'estudiar els fòssils. El segon, ens ajuda a veure, amb exemples diversos, les limitacions d'aquesta ciència i ens fa entendre la seva contínua construcció i evolució a partir de les noves dades aportades per les descobertes que contínuament fan els paleontòlegs. Aquest paquet pertany al conjunt d'aplicacions sobre l'evolució format per Petjades d'home, Canviar per viure, Una gran família, Bricolatge còsmic, Refent el passat i Ombres del passat. Trobareu més informació a la web Petjades d'home: un viatge per la nostra evolució", "Català", "refent/refent.jclic.zip");INSERT INTO zip_urls (id, url) VALUES ("1469", "http://clic.xtec.net/projects/refent/jclic/refent.jclic.zip");INSERT INTO img_urls (id, url) VALUES ("1469", "http://clic.xtec.net/gifs/refent.gif");INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo) VALUES ("1470 1287", "ombres.gif", "Ombres del passat", "Secundaria", "Ciències socials", "Paquet d'activitats on se'ns presenta els fòssils com a prova més clara i contundent de l'existència de l'evolució de les espècies. La realització de les diverses activitats permetrà al alumne adonar-se que les espècies han anat canviant al llarg de la història de la vida. Aquest paquet pertany al conjunt d'aplicacions sobre l'evolució format per Petjades d'home, Canviar per viure, Una gran família, Bricolatge còsmic, Refent el passat i Ombres del passat. Trobareu més informació a la web Petjades d'home: un viatge per la nostra evolució.", "Català", "ombres/ombres.jclic.zip");INSERT INTO zip_urls (id, url) VALUES ("1470 1287", "http://clic.xtec.net/projects/ombres/jclic/ombres.jclic.zip http://clic.xtec.net/projects/nombres/jclic/nombres.jclic.zip http://clic.xtec.net/projects/numeros3/jclic/numeros3.jclic.zip http://clic.xtec.net/projects/zenbaize/jclic/zenbaize.jclic.zip");INSERT INTO img_urls (id, url) VALUES ("1470 1287", "http://clic.xtec.net/gifs/ombres.gif http://clic.xtec.net/gifs/nombres.gif");INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo) VALUES ("1471", "plestny3.gif", "El Pla de l'Estany", "Primaria, Secundaria", "Ciències socials", "Paquet d'activitats sobre la comarca del Pla de l'Estany on es treballa: Història de la comarca Els municipis Geografia física i política La població L'estany de Banyoles", "Català", "plestny3/plestny3.jclic.zip");INSERT INTO zip_urls (id, url) VALUES ("1471", "http://clic.xtec.net/projects/plestny3/jclic/plestny3.jclic.zip");INSERT INTO img_urls (id, url) VALUES ("1471", "http://clic.xtec.net/gifs/plestny3.gif");INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo) VALUES ("1472", "histori2.gif", "Orígens de la humanitat i antigues civilitzacions", "Primaria, Secundaria", "Ciències socials", "Paquet d'activitats basat en el llibre 'Geografía e historia' de l'editorial SM, destinat a alumnes de 3r cicle de primària i 1r d'ESO, on es treballen dos temes diferents: Els orígens de la humanitat: La prehistòria i les seves grans etapes, l'aparició dels primers homes i dones, la seva vida, per què pintaven les homes i dones del paleolític i la gran evolució de la cultura neolítica. Antigues civilitzacions e la mediterrània: Egipte, els pobles Mesopotàmics (Assíria i Babilònia), les civilitzacions persa i fenícia.", "Català", "histori2/histori2.jclic.zip");INSERT INTO zip_urls (id, url) VALUES ("1472", "http://clic.xtec.net/projects/histori2/jclic/histori2.jclic.zip");INSERT INTO img_urls (id, url) VALUES ("1472", "http://clic.xtec.net/gifs/histori2.gif");INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo) VALUES ("1473", "histori3.gif", "Grècia i Roma", "Primaria, Secundaria", "Ciències socials", "Paquet d'activitats basat en el llibre 'Geografía e historia' de l'editorial SM, destinat a alumnes de 3r cicle de primària i 1r d'ESO, on es treballen dos temes diferents: L'herència clàssica: Grècia Les ciutats gregues: Esparta i Atenes, la lluita dels grecs contra els perses, el predomini de Macedònia i la cultura, l'art i les civilitzacions. L'herència clàssica: Roma Orígens de Roma, expansió de Roma: el seu gran imperi, la república romana, els grans emperadors romans i el seu llegat: dret, idioma i art.", "Català", "histori3/histori3.jclic.zip");INSERT INTO zip_urls (id, url) VALUES ("1473", "http://clic.xtec.net/projects/histori3/jclic/histori3.jclic.zip http://clic.xtec.net/projects/histo3es/jclic/histo3es.jclic.zip");INSERT INTO img_urls (id, url) VALUES ("1473", "http://clic.xtec.net/gifs/histori3.gif");INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo) VALUES ("1473", "histori3.gif", "Grècia i Roma", "Primaria (6-12), Secundaria (12-16)", "Ciencias sociales", "Paquete de actividades basado en el libro 'Geografía e historia' de la editorial SM, destinado a alumnos de 3r ciclo de primaria y 1º de ESO, dónde se trabajan dos temas diferentes: La herencia clásica: Grecia Las ciudades griegas: Esparta y Atenas, la lucha de griegos y persas, el predominio de Macedonia y la cultura , el arte y las civilizaciones. La herencia clásica: Roma Orígenes de Roma, expansión de Roma: su gran imperio, la república romana, los grandes emperadores y su legado: derecho, idioma y arte.", "Español", "histo3es/histo3es.jclic.zip");INSERT INTO zip_urls (id, url) VALUES ("1473", "http://clic.xtec.net/projects/histori3/jclic/histori3.jclic.zip http://clic.xtec.net/projects/histo3es/jclic/histo3es.jclic.zip");INSERT INTO img_urls (id, url) VALUES ("1473", "http://clic.xtec.net/gifs/histori3.gif");INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo) VALUES ("1474", "arteesp.gif", "Arte Español", "Primaria, Secundaria", "Ciencias sociales", "Este paquete de actividades es un breve recorrido por algunas de las obras básicas del arte Español de todos los tiempos, desde la prehistoria hasta la hasta la Edad Media. Algunas de las obras que se trabajan son: Cuevas de Altamira Dama de Elche Alhambra Catedral de Burgos Mezquita de Córdoba Sant Climent de Taüll El paquete de actividades está destinado a alumnos de Ciclo Medio de Educación Primaria.", "Español", "arteesp/arteesp.jclic.zip");INSERT INTO zip_urls (id, url) VALUES ("1474", "http://clic.xtec.net/projects/arteesp/jclic/arteesp.jclic.zip");INSERT INTO img_urls (id, url) VALUES ("1474", "http://clic.xtec.net/gifs/arteesp.gif");INSERT INTO jclic (id,image,actividad,edad,materia,descripcion,idioma,archivo) VALUES ("1475", "aragfis.gif", "Aragón: Geografía Física", "Primaria, Secundaria", "Ciencias sociales", "Paquete de actividades sobre la Geografía Física de Aragón: situación y límites, macizos montañosos, ríos y afluentes, lagunas e ibones... organizadas a partir de las tres grandes unidades físicas de la c